    <bpmn:scriptTask id="assess_loan_risk" name="Assess Loan Risk" scriptFormat="Groovy">
      <bpmn:incoming>SequenceFlow_1rp5sx0</bpmn:incoming>
      <bpmn:outgoing>SequenceFlow_1iilpl2</bpmn:outgoing>
      <bpmn:script><![CDATA[
if(credit_assessment=='AAA'){
	execution.setVariable ("risk_weight",0);
} else if(credit_assessment=='AA'){
	execution.setVariable ("risk_weight",20);
}else if(credit_assessment=='A'){
	execution.setVariable ("risk_weight",40);
}else if(credit_assessment=='BBB'){
	execution.setVariable ("risk_weight",60);
}else if(credit_assessment=='BB'){
	execution.setVariable ("risk_weight",80);
}else if(credit_assessment=='B'){
	execution.setVariable ("risk_weight",100);
}]]></bpmn:script>
